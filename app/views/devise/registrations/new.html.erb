<div class="containerSession">
  <div class="col-sm-6 col-sm-offset-3">
      <h2>Registro</h2>

      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>

        <div class="field">
          <%= f.label :nombre_de_usuario %><br />
          <%= f.text_field :username, autofocus: true %>
        </div>

        <div class="field">
          <%= f.label :Primer_Nombre %><br />
          <%= f.text_field :name1 %>
        </div>

        <div class="field">
          <%= f.label :Segundo_Nombre %><br />
          <%= f.text_field :name2 %>
        </div>

        <div class="field">
          <%= f.label :Primer_Apellido %><br />
          <%= f.text_field :ape1 %>
        </div>

        <div class="field">
          <%= f.label :Segundo_Apellido %><br />
          <%= f.text_field :ape2 %>
        </div>

        <div class="field">
          <%= f.label :email %><br />
          <%= f.email_field :email %>
        </div>

        <div class="field">
          <%= f.label :Contraseña %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "off" %>
        </div>

        <div class="field">
          <%= f.label :Confirmación_de_Contraseña %><br />
          <%= f.password_field :password_confirmation, autocomplete: "off" %>
        </div>

        <div class="actions">
          <%= f.submit "Registrarme" %>
        </div>
      <% end %>

      <%= render "devise/shared/links" %>
  </div>
</div>
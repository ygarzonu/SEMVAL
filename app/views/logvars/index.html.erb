<p id="notice"><%= notice %></p>

<h1>Listing Logvars</h1>

<table>
  <thead>
    <tr>
      <th>Valor</th>
      <th>Fechad</th>
      <th>Variable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logvars.each do |logvar| %>
      <tr>
        <td><%= logvar.valor %></td>
        <td><%= logvar.fechaD %></td>
        <td><%= logvar.variable %></td>
        <td><%= link_to 'Show', logvar %></td>
        <td><%= link_to 'Edit', edit_logvar_path(logvar) %></td>
        <td><%= link_to 'Destroy', logvar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Logvar', new_logvar_path %>
